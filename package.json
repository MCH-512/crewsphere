{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next dev:emulators",
    "dev:core": "npm-run-all --parallel dev:next emulators:core",
    "dev:next": "next dev --port 9002 --hostname 0.0.0.0",
    "dev:emulators": "firebase emulators:start --only functions,firestore,hosting,extensions",
    "emulators:core": "firebase emulators:start --only firestore,functions,hosting",
    "build": "next build",
    "start": "next start --port 9002 --hostname 0.0.0.0",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "audit": "node nextjs-audit.js",
    "audit:ai": "node src/scripts/run-code-audit.js",
    "test": "node src/scripts/run-all-tests.js",
    "test:unit": "jest --coverage",
    "test:e2e": "playwright test",
    "test:accessibility": "playwright test tests/accessibility/accessibility.spec.ts",
    "test:ai": "jest --testPathPattern=tests/ai-validation",
    "test:auto-generate": "node src/scripts/generate-tests.js",
    "auto-optimize": "node src/scripts/run-analysis.js"
  },
